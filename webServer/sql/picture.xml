<?xml version="1.0" encoding="gb2312"?>
<database name="jade_db" character="utf8" collate="utf8_general_ci">
    <tables tablePrefix="t_">
        <table name="picture" engine='InnoDB' charset='latin1' row_format='COMPRESSED' key_block_size='8' desc="图片">
            <field name="id" type="bigint" length="20" null="false" autoincr="true" start="1" comment="唯一ID"/>
            <field name="name" type="varchar" length="128" null="false" default="" character='utf8' collate='utf8_bin' comment="图片名"/>
            <field name="content" type="varchar" length="65535" null="false" default="0" comment="图片内容"/>
            <field name="tm" type="int" length="10" null="false" default='0' comment="时间"/>
            <field name="create_tm" type="timestamp" null="false"
                   default="CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP" comment="最后更新时间"/>
            <index type="PRIMARY KEY" field="id"/>
            <index type="KEY" name="name" field="name" using="BTREE"/>
            <index type="KEY" name="tm" field="tm" using="BTREE"/>
        </table>
    </tables>
</database>